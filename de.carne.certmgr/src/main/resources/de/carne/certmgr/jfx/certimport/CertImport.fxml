            <TreeTableColumn fx:id="ctlImportSelectionHasCRT" editable="false" prefWidth="-1.0" sortable="false" text="%label.importselectionhascrt" />
            <TreeTableColumn fx:id="ctlImportSelectionHasCSR" editable="false" prefWidth="-1.0" sortable="false" text="%label.importselectionhascsr" />
