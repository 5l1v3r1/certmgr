                     <image>
                        <Image url="@../iconFile16.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
         </children>
      </HBox>
      <RadioButton fx:id="ctlURLSourceOption" layoutX="14.0" layoutY="47.0" mnemonicParsing="false" text="%label.url" toggleGroup="$source" GridPane.rowIndex="2" />
      <TextField fx:id="ctlURLSourceInput" GridPane.columnIndex="1" GridPane.rowIndex="2" />
      <RadioButton fx:id="ctlClipboardSourceOption" layoutX="14.0" layoutY="76.0" mnemonicParsing="false" text="%label.clipboard" toggleGroup="$source" GridPane.columnSpan="2147483647" GridPane.rowIndex="3" />
