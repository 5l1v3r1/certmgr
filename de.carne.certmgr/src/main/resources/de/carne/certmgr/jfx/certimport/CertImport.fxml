               </children>
            </HBox>
            <Button cancelButton="true" minWidth="80.0" mnemonicParsing="false" onAction="#onReload" text="%button.reload" />
         </children>
      </HBox>
      <HBox spacing="4.0" GridPane.columnSpan="2147483647" GridPane.rowIndex="5">
         <children>
            <ImageView fx:id="ctlImportStatusIcon" pickOnBounds="true" preserveRatio="true" HBox.hgrow="NEVER">
               <image>
                  <Image url="@../iconInfo16.png" />
               </image>
            </ImageView>
            <Label fx:id="ctlImportStatusMessage" text="%certimport.setupsource" HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
      <Separator prefWidth="200.0" GridPane.columnSpan="2" GridPane.rowIndex="6" />
      <Label layoutX="14.0" layoutY="220.0" text="%label.importselection" GridPane.columnSpan="2147483647" GridPane.rowIndex="7" />
