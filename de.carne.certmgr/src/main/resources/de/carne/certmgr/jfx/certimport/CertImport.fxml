      <HBox GridPane.columnIndex="1">
         <children>
            <TextField fx:id="ctlFileSourceInput" GridPane.columnIndex="1" GridPane.rowIndex="1" HBox.hgrow="ALWAYS" />
            <Button fx:id="ctlChooseFileSourceButton" mnemonicParsing="false" onAction="#onChooseFileSource">
               <graphic>
                  <ImageView pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../iconFile16.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
         </children>
      </HBox>
      <RadioButton fx:id="ctlFolderSourceOption" layoutX="14.0" layoutY="18.0" mnemonicParsing="false" text="%label.folder" toggleGroup="$source" GridPane.rowIndex="1" />
