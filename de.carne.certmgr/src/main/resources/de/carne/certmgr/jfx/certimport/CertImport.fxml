<GridPane hgap="8.0" vgap="8.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.carne.certmgr.jfx.certimport.CertImportController">
  <children>
      <RadioButton fx:id="ctlFileSourceOption" mnemonicParsing="false" text="%label.file">
